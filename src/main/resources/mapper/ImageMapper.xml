<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.medirec.mapper.ImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.medirec.entity.Image">
        <id column="user_id" property="userId" />
        <result column="id" property="id" />
        <result column="pat_id" property="patId" />
        <result column="file_name" property="fileName" />
        <result column="sinogram" property="sinogram" />
        <result column="suffix" property="suffix" />
        <result column="height" property="height" />
        <result column="width" property="width" />
        <result column="input_url" property="inputUrl" />
        <result column="demo_url" property="demoUrl" />
        <result column="output_url" property="outputUrl" />
        <result column="upload_time" property="uploadTime" />
    </resultMap>

</mapper>
